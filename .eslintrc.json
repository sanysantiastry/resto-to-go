{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "linebreak-style": 0,
    "quotes": 0,
    "no-else-return": 0,
    "comma-dangle": 0,
    "semi": 0,
    "operator-linebreak": 0,
    "no-restricted-syntax": 0,
    "prefer-destructuring": 0,
    "no-alert": 0,
    "no-unused-vars": 0,
    "space-before-function-paren": 0,
    "arrow-parens": 0,
    "no-undef": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "no-extra-semi": 0,
    "no-use-before-define": 0,
    "no-plusplus": 0,
    "no-await-in-loop": 0,
    "no-new": 0
  }
}
